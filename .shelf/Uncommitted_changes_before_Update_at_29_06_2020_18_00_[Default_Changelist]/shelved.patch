<+>html, body {\n    height: 100%;\n}\n\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\n/** Basic styling **/\np {\n    font-size: 15px;\n    margin: 7px;\n}\n\ndetails {\n    padding: 5px 5px 0;\n}\n\nvar {\n    font-style: normal;\n}\n\n/* Overflow if the text is too long */\nbody .lm_content {\n    overflow: scroll;\n}\n\n[onClick] {\n    cursor: pointer;\n}\n\n[onClick]:hover {\n    /*color: #E55B0A;*/\n    color: #d96629;\n}\n\n/** Wrapper **/\n#wrapper {\n    height: 100%;\n    position: relative;\n    width: 100%;\n    overflow: hidden;\n}\n\n/** Menu **/\n#menuContainer {\n    width: 20%;\n    height: 100%;\n    position: absolute;\n    top: 0;\n    left: 0;\n    background: #222;\n    list-style-type: none;\n}\n\n#menuContainer h1 {\n    border-bottom: 1px solid #000;\n    border-top: 1px solid #333;\n    padding: 10px 5px;\n    color: #BBB;\n    background: #1a1a1a;\n    font: 24px Arial, sans-serif;\n    text-align: center;\n}\n\n#menuContainer h2 {\n    border-bottom: 1px solid #000;\n    border-top: 1px solid #333;\n    padding: 10px 5px;\n    color: #BBB;\n    background: #1a1a1a;\n    font: 20px Arial, sans-serif;\n    text-align: center;\n}\n\n#menuContainer h3 {\n    cursor: pointer;\n    border-top: 1px solid #333;\n    padding: 10px 5px;\n    color: #BBB;\n    background: #222;\n    font: 15px Arial, sans-serif;\n    text-align: center;\n    font-style: italic;\n}\n\n#menuContainer li {\n    border-bottom: 1px solid #000;\n    border-top: 1px solid #333;\n    padding: 10px 5px;\n    color: #BBB;\n    background: #1a1a1a;\n    font: 12px Arial, sans-serif;\n    cursor: pointer;\n\n}\n\n#menuContainer li:hover {\n    background: #111;\n    color: #CCC;\n}\n\n/** Layout **/\n#layoutContainer {\n    color: #fff;\n    width: 80%;\n    height: 100%;\n    position: absolute;\n    top: 0;\n    left: 20%;\n    box-shadow: -3px 0 9px 0 rgba(0, 0, 0, 0.4);\n    font-family: Arial, sans-serif;\n    background-color: #222222;\n}\n\n#layoutContainer .container {\n    margin: 15px 10px 10px 10px;\n}\n\n#layoutContainer h1 {\n    font-size: 20px;\n}\n\n#layoutContainer h2 {\n    font-size: 18px;\n}\n\n#layoutContainer h3 {\n    font-size: 16px;\n}\n\n#layoutContainer td {\n    padding: 3px;\n}\n\n#layoutContainer button {\n    padding: 0.2rem;\n    display: inline-block;\n    margin: 10px;\n}\n\n#layoutContainer table {\n    margin-top: 10px;\n    margin-bottom: 10px;\n}\n\n#layoutContainer ul {\n    list-style-position: inside;\n    padding: 7px;\n}\n\n#layoutContainer ol {\n    counter-reset: node;\n    list-style: none;\n}\n\n#layoutContainer li {\n    font-size: 15px;\n    padding: 7px;\n}\n\n#layoutContainer #blocks_list li {\n    list-style-type: '# ';\n}\n\n#layoutContainer #nodes_list li::before {\n    counter-increment: node;\n    content: \"Node \" counter(node) \": \";\n}\n\n/* Style inputs, select elements and textareas */\n#layoutContainer input[type=text], select, textarea {\n    width: 100%;\n    padding: 5px;\n    margin: 10px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    box-sizing: border-box;\n    resize: vertical;\n}\n\n#layoutContainer input[type=number] {\n    width: 150px;\n    margin: 10px;\n    padding: 5px;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    box-sizing: border-box;\n    resize: vertical;\n}\n\n/* Style the label to display next to the inputs */\n#layoutContainer label {\n    padding: 12px 12px 12px 0;\n    display: inline-block;\n}\n\n#sellNew_receipt {\n    text-align: center;\n    font-style: italic;\n    font-size: 15px;\n    margin-bottom: 5px;\n    margin-left: auto;\n    margin-right: auto;\n}\n\n/** Messages to the user **/\n.message {\n    text-align: center;\n    font-style: italic;\n    font-size: 15px;\n    margin-bottom: 5px;\n}\n\n.link {\n    font-size: 13px;\n    margin: 7px;\n    font-style: italic;\n}\n\n/* Floating column for labels: 25% width */\n.col-25 {\n    float: left;\n    width: 25%;\n    margin-top: 6px;\n}\n\n/* Floating column for inputs: 75% width */\n.col-75 {\n    float: left;\n    width: 75%;\n    margin-top: 6px;\n}\n\n/* Clear floats after the columns */\n.row:after {\n    content: \"\";\n    display: table;\n    clear: both;\n}\n\n/** To delete? **/\n/* Only serves to close the server */\na {\n    text-decoration: none;\n    color: #BBB;\n}
